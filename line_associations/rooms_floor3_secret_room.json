        "en_clean": "Drama!",
        "jp_raw": "\\m[vi]劇的だなあ！",
        "jp_clean": "劇的だなあ！"
    },
    {
        "en_raw": "They ran away from home?! Why would they do that?!",
        "en_clean": "They ran away from home?! Why would they do that?!",
        "jp_raw": "\\m[vb]家を飛び出したの！？\\!\\ どうしてそんなことを！？",
        "jp_clean": "家を飛び出したの！？ どうしてそんなことを！？"
    },
